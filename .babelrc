{
    "env": {
        "test": {
            "presets": [[
                "@babel/env"
            ], [
                "@babel/react"
            ]]
        },
        "development": {
            "plugins": [[
                "@babel/proposal-object-rest-spread"
            ]]
        },
        "production": {
            "plugins": [[
                "@babel/proposal-object-rest-spread"
            ]]
